<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <connectionStrings>
    <add name="CarRentalContext" providerName="System.Data.SqlClient" connectionString="Data Source=.;Initial Catalog=CarRentalDB;Integrated Security=True" />
  </connectionStrings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="CarServiceSoap" />
        <binding name="RentalInfoServiceSoap" />
        <binding name="UserServiceSoap" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:50245/CarService.asmx" binding="basicHttpBinding"
        bindingConfiguration="CarServiceSoap" contract="CarRentalCarService.CarServiceSoap"
        name="CarServiceSoap" />
      <endpoint address="http://localhost:50245/RentalInfoService.asmx"
        binding="basicHttpBinding" bindingConfiguration="RentalInfoServiceSoap"
        contract="CarRentalRentalInfoService.RentalInfoServiceSoap"
        name="RentalInfoServiceSoap" />
      <endpoint address="http://localhost:50245/UserService.asmx" binding="basicHttpBinding"
        bindingConfiguration="UserServiceSoap" contract="CarRentalUserService.UserServiceSoap"
        name="UserServiceSoap" />
    </client>
  </system.serviceModel>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlConnectionFactory, EntityFramework" />
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
</configuration>